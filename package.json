{
  "name": "ngrx-signal-store-examples",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "test": "ng test",
    "serve:ssr:ngrx-signal-store-examples": "node dist/ngrx-signal-store-examples/server/server.mjs",
    "lint": "ng lint",
    "ci": "ng lint && ng test && ng build",
    "format:test": "prettier --list-different \"./src/**/*.{ts,html,scss,json}\"",
    "format:write": "prettier --write \"./src/**/*.{ts,html,scss,json}\"",
    "analyze": "ng build --stats-json --output-hashing=none --named-chunks && esbuild-visualizer --template treemap --metadata dist/ngrx-signal-store-examples/stats.json --filename dist/ngrx-signal-store-examples/analyse/index.html  && http-server -o -c-1 ./dist/ngrx-signal-store-examples/analyse/",
    "analyze:sme": "ng build --source-map --output-hashing=none --named-chunks && source-map-explorer dist/ngrx-signal-store-examples/browser/*.js --html dist/ngrx-signal-store-examples/sme/index.html && http-server -o -c-1 ./dist/ngrx-signal-store-examples/sme/",
    "analyze:deps": "npm-run-all analyze:deps:*",
    "analyze:deps:all": "madge ./src/main.ts --ts-config tsconfig.json --image ./deps/_all.png",
    "analyze:deps:ui": "madge  --extensions ts --ts-config tsconfig.json --image ./deps/ui.png /src/app/ui/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.2.1",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/material": "^17.2.1",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/platform-server": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@angular/ssr": "^17.0.7",
    "@ngrx/entity": "^17.1.0",
    "@ngrx/signals": "^17.1.0",
    "express": "^4.18.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.7",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "^17.0.7",
    "@angular/compiler-cli": "^17.0.0",
    "@hirez_io/observer-spy": "^2.2.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "autoprefixer": "^10.4.19",
    "esbuild-visualizer": "^0.6.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-boundaries": "^4.2.0",
    "http-server": "^14.1.1",
    "jasmine-auto-spies": "^8.0.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "madge": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.2.2"
  }
}
